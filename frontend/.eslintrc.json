{
  "plugins": ["prettier"],
  "extends": ["prettier", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "tsx": true
    }
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "comma-dangle": 0
  }
}
